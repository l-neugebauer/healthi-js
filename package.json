{
  "name": "healthi",
  "version": "1.0.8",
  "description": "Simple module to get your Macbook's battery health",
  "main": "bin/healthi.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir bin",
    "prepublish": "yarn run build",
    "prepush": "yarn run test",
    "test": "yarn run build && xo && ava"
  },
  "keywords": [
    "health"
  ],
  "author": "Pablo Varela",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "husky": "^0.13.3",
    "xo": "^0.19.0"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": true,
    "ignores": [
      "bin/**"
    ],
    "rules": {
      "new-cap": 0
    }
  },
  "dependencies": {
    "shelljs": "^0.7.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablopunk/healthi-js"
  }
}
