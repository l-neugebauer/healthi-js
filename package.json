{
  "name": "healthi",
  "version": "2.0.2",
  "description": "Simple module to get your Macbook's battery health",
  "main": "build.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file build.js",
    "prepublish": "yarn run build && yarn run test",
    "prepush": "yarn run build && yarn run test",
    "test": "standard --fix index.js && ava"
  },
  "keywords": [
    "health",
    "mac",
    "macbook",
    "battery"
  ],
  "author": "Pablo Varela",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.0",
    "husky": "^0.13.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "shelljs": "^0.7.6",
    "standard": "^10.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablopunk/healthi-js"
  }
}
