{
  "name": "healthi",
  "version": "3.1.0",
  "description": "Simple module to get your Macbook's battery health",
  "main": "build.js",
  "scripts": {
    "build": "babel index.js --out-file build.js",
    "prepublish": "yarn run build && yarn run test",
    "prepush": "yarn run build && yarn run test",
    "test": "standard --fix index.js && ava"
  },
  "keywords": [
    "health",
    "mac",
    "macbook",
    "battery"
  ],
  "author": "Pablo Varela",
  "license": "MIT",
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "electron": "1.4"
        }
      }]
    ]
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-preset-env": "^1.6.0",
    "husky": "^0.13.3",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "shelljs": "^0.7.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablopunk/healthi-js"
  }
}
