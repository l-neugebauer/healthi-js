{
  "name": "healthi",
  "version": "4.0.0",
  "description": "Simple module to get your PC's battery health",
  "main": "build.js",
  "scripts": {
    "build": "babel index.js --out-file build.js",
    "prepublish": "npm run build && npm run test",
    "prepush": "npm run build && npm run test",
    "test": "npm run build && standard --fix index.js && ava"
  },
  "keywords": [
    "health",
    "mac",
    "pc",
    "macbook",
    "battery"
  ],
  "author": "Pablo Varela",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "electron": "1.4"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "husky": "^0.13.3",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "execa": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablopunk/healthi-js"
  }
}
